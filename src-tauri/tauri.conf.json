{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "GlobalVimLikeNavi",
  "version": "0.2.1",
  "identifier": "me.xueshi.globalvimlikenavi",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1421",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Global Vim-Like Navi",
        "width": 666,
        "height": 1100,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
